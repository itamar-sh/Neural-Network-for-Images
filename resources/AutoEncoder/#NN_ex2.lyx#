\begin_layout Standard
\begin_inset Formula $(d$
\end_inset

 
\begin_inset Formula 
\[
f(x+g(x+h(x)))
\]

\end_inset


\end_layout

\begin_layout Standard
נפרק כל ביטוי כפול סכום הנגזרות של הפונקציות הפנימיות:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\text{\frac{df}{dx}=\frac{\partial}{\partial x}\ensuremath{f(x+g(x+h(x))\cdot(1+\frac{\partial}{\partial x}g(x+h(x)))\cdot(1+\frac{\partial}{\partial x}h(x))}}
\]

\end_inset


\end_layout

